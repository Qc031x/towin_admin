<script type="text/javascript">
    $(document).ready(function(){
        getdata();
        getResult();
        binNum()
    });


    function getResult(){
        $(".refresh").click(function () {
            getdata();
            binNum()
        })
    }

    function getdata(){
        $(".fanguan").each(function(){
            var gunid =$(this).find(".t_num").attr("id");
            var nums = Number($("#"+gunid).attr("oldnumber"));
            show_num(gunid,nums);
        });
    }

    function show_num(gun,n){
        $("#"+gun+" u").remove();
        var it = $("#"+gun+" u");
        var len = String(n).length;
        for(var i=0;i<len;i++){
            if(it.length<=i){
                $("#"+gun).append("<u>0<br>1<br>2<br>3<br>4<br>5<br>6<br>7<br>8<br>9</u>");
            }
            var num=String(n).charAt(i);
            var y = -parseInt(num)*45;
            var obj = $("#"+gun+" u").eq(i);
            obj.animate({
                        top :String(y)+'px'
                    }, 'slow','swing',function(){}
            );
        }
        $("#"+gun).attr("oldnumber",n)
    }

    function binNum() {
        var bigNum = Number($("#t_num3").attr("oldnumber"));
        if(bigNum > 99999){
            $(".menulink .link3 .t_num u").addClass("active");
        }
    }

</script>
