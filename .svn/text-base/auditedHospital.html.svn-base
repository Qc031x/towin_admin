		<script>
			$(document).ready(function() {
				$(".tcdPageCode").css('display', 'block');

				//右侧边栏内容高度计算
				$('.checkdetail .content').css('height', $('.checkdetail').height() - $('.manup').height() - $('.checkdetail .hd').height() + 'px');

				$('.checkchose .pass').click(function() {
					$(this).addClass('colg1').siblings(".refuse").removeClass('colg2');
					$('.price').show();
					$('.reason').hide();
				});
				$('.checkchose .refuse').click(function() {
					$(this).addClass('colg2').siblings(".pass").removeClass('colg1');
					$('.reason').show();
					$('.price').hide();
				});

				//右侧边栏预约主体切换
				$('.cek').click(function() {
					$(this).parent().parent().addClass('trActive');
					var format = $(this).parent().siblings(".cng").html();
					$(".blFormat").html(format);
					$('.checkdetail').css('display', 'block');

				});

				//右侧边栏底部操作
				$('.undo').click(function() {
					$('.checkdetail').css('display', 'none');
				});
			});
		</script>

